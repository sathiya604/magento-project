<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <!-- here we remove virtualType and defile collection as follow-->
    <type name="TrendsAttribute\AdminSales\Model\ResourceModel\User\Grid\Collection">
        <arguments>
            <argument name="mainTable" xsi:type="string">sales_order</argument>
            <argument name="eventPrefix" xsi:type="string">trendsattribute_adminsales_grid_collection</argument>
            <argument name="eventObject" xsi:type="string">trendsattribute_adminsales_records_collection</argument>
            <argument name="resourceModel" xsi:type="string">TrendsAttribute\AdminSales\Model\ResourceModel\Sales</argument>
        </arguments>
    </type>
    <type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <!--data provider name which used in grid ui component file -->
                <item name="trendsattribute_adminsales_grid_data_source" xsi:type="string">TrendsAttribute\AdminSales\Model\ResourceModel\User\Grid\Collection</item>
            </argument>
        </arguments>
    </type>
    <type name='Magento\Quote\Model\Quote\Item\ToOrderItem'>
        <plugin name='AddclothingMaterial' type='TrendsAttribute\AdminSales\Plugin\Model\Quote\Item\ToOrderItem' sortOrder='99'/>
    </type>

</config>